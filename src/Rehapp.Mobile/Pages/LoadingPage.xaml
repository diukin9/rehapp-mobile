<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"  
             x:Class="Rehapp.Mobile.Pages.LoadingPage">

    <ContentPage.Behaviors>
        <toolkit:StatusBarBehavior StatusBarColor="#3A3A3A" StatusBarStyle="LightContent" />
    </ContentPage.Behaviors>

    <StackLayout>
        <ActivityIndicator IsRunning="True" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" Color="#FF8787"/>
    </StackLayout>
    
</ContentPage>